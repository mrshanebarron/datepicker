{
    "name": "mrshanebarron/datepicker",
    "description": "Date and time picker component using Flatpickr for Laravel with Livewire, Blade, and Vue support",
    "keywords": ["laravel", "datepicker", "flatpickr", "datetime", "calendar", "livewire", "blade", "vue"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shane Barron",
            "email": "mrshanebarron@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelDesign\\Datepicker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelDesign\\Datepicker\\DatepickerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
